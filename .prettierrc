{
    "singleQuote": false,
    "printWidth": 100,
    "editor.formatOnSave": true,
    "proseWrap": "always",
    "tabWidth": 4,
    "indent_size": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "trailingComma": "none",
    "arrowParens": "always",
    "semi": false,
    "semicolons": true,
    "htmlWhitespaceSensitivity": "strict"
}
